{
  "name": "games-room-browser",
  "version": "0.0.1",
  "description": "Games Room - Browser",
  "main": "src/index.tsx",
  "author": {
    "name": "Aleksander Fret",
    "email": "olekfret80@gmail.com",
    "url": "https://github.com/aleksanderfret/"
  },
  "homepage": "https://github.com/aleksanderfret/games-room/tree/master/packages/browser#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/aleksanderfret/games-room/tree/master/packages/browser"
  },
  "bugs": {
    "url": "https://github.com/aleksanderfret/games-room/issues"
  },
  "scripts": {
    "build": "webpack --progress --config ./webpack.config.prod.ts",
    "check": "npm-run-all lint format stylelint test compile",
    "compile": "tsc -p ./tsconfig.prod.json",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "libs": "license-checker-rseidelsohn --json --out libraries.json",
    "lint": "eslint . --ext \".js,.jsx,.ts,.tsx\"",
    "lint:fix": "eslint ./ --ext \".js,.jsx,.ts,.tsx\" --max-warnings=0 --fix",
    "pre-commit": "lint-staged && npm run test",
    "pre-push": "npm-run-all check test",
    "snapshot:update": "npm test -- -u",
    "start": "webpack serve --mode development --config ./webpack.config.dev.ts",
    "storybook": "start-storybook -p 6006",
    "stylelint": "npm-run-all stylelint:styled",
    "stylelint:styled": "stylelint src/**/*.tsx --cache",
    "test": "jest --runInBand --verbose --passWithNoTests",
    "test:watch": "jest --watch",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@formatjs/intl-pluralrules": "4.2.0",
    "@formatjs/intl-relativetimeformat": "9.4.0",
    "apollo-link-token-refresh": "0.3.3",
    "axios": "0.24.0",
    "clsx": "1.1.1",
    "graphql": "15.7.2",
    "jwt-decode": "3.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.0",
    "react-query": "3.34.7",
    "react-router-dom": "6.2.1",
    "styled-components": "5.3.3",
    "ts-polyfill": "3.8.2",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@react-theming/storybook-addon": "1.1.3",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/builder-webpack5": "6.4.9",
    "@storybook/manager-webpack5": "6.4.9",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.4.9",
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/jest": "27.4.0",
    "@types/mini-css-extract-plugin": "2.4.0",
    "@types/node": "17.0.6",
    "@types/optimize-css-assets-webpack-plugin": "5.0.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/styled-components": "5.1.19",
    "@types/terser-webpack-plugin": "5.2.0",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-dev-server": "4.5.1",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "babel-loader": "8.2.3",
    "concurrently": "6.5.1",
    "copy-webpack-plugin": "10.2.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest-dom": "4.0.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-testing-library": "5.0.1",
    "fork-ts-checker-webpack-plugin": "6.5.0",
    "graphql-tag": "2.12.6",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.5",
    "jest-styled-components": "7.0.8",
    "license-checker-rseidelsohn": "2.2.0",
    "lint-staged": "12.1.4",
    "minify-graphql-loader": "1.0.2",
    "msw": "0.36.3",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "prettier": "2.5.1",
    "storybook-addon-styled-component-theme": "2.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "terser-webpack-plugin": "5.3.0",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "ttypescript": "1.5.13",
    "typescript": "4.5.4",
    "typescript-plugin-styled-components": "2.0.0",
    "webpack": "5.65.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not IE 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,scss,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc --fix",
      "echo test"
    ],
    "*.{jsx,tsx}": [
      "npm run stylelint:styled"
    ]
  }
}
