{
  "name": "games-room-browser",
  "version": "0.0.1",
  "description": "Games Room - Browser",
  "main": "src/index.tsx",
  "author": {
    "name": "Aleksander Fret",
    "email": "olekfret80@gmail.com",
    "url": "https://github.com/aleksanderfret/"
  },
  "homepage": "https://github.com/aleksanderfret/games-room/tree/master/packages/browser#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/aleksanderfret/games-room/tree/master/packages/browser"
  },
  "bugs": {
    "url": "https://github.com/aleksanderfret/games-room/issues"
  },
  "scripts": {
    "build": "webpack --progress --config ./webpack.config.prod.ts",
    "check": "npm-run-all lint format stylelint test compile",
    "compile": "tsc -p ./tsconfig.prod.json",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "libs": "license-checker-rseidelsohn --json --out libraries.json",
    "lint": "eslint . --ext \".js,.jsx,.ts,.tsx\"",
    "lint:fix": "eslint ./ --ext \".js,.jsx,.ts,.tsx\" --max-warnings=0 --fix",
    "pre-commit": "lint-staged && npm run test",
    "pre-push": "npm-run-all check test",
    "snapshot:update": "npm test -- -u",
    "start": "webpack serve --mode development --config ./webpack.config.dev.ts",
    "storybook": "start-storybook -p 6006",
    "stylelint": "npm-run-all stylelint:styled stylelint:scss",
    "stylelint:styled": "stylelint src/**/*.tsx --cache --config ./stylelint.styled.json",
    "stylelint:scss": "stylelint src/**/*.scss --cache --config ./stylelint.scss.json",
    "test": "jest --runInBand --verbose --passWithNoTests",
    "test:watch": "jest --watch",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "4.1.4",
    "@formatjs/intl-relativetimeformat": "9.3.1",
    "axios": "0.23.0",
    "clsx": "1.1.1",
    "jwt-decode": "3.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.20.12",
    "react-query": "3.27.0",
    "react-router-dom": "5.3.0",
    "styled-components": "5.3.1",
    "ts-polyfill": "3.8.2",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@react-theming/storybook-addon": "1.1.1",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.3.10",
    "@testing-library/dom": "8.9.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.4.1",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/jest": "27.0.2",
    "@types/mini-css-extract-plugin": "2.3.0",
    "@types/node": "16.11.0",
    "@types/optimize-css-assets-webpack-plugin": "5.0.4",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.3.1",
    "@types/styled-components": "5.1.15",
    "@types/terser-webpack-plugin": "5.2.0",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-dev-server": "4.3.1",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "autoprefixer": "10.3.7",
    "babel-loader": "8.2.2",
    "concurrently": "6.3.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.4.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.12.4",
    "fork-ts-checker-webpack-plugin": "6.3.4",
    "html-webpack-plugin": "5.3.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.2.5",
    "jest-styled-components": "7.0.5",
    "license-checker-rseidelsohn": "2.2.0",
    "lint-staged": "11.2.3",
    "mini-css-extract-plugin": "2.4.2",
    "msw": "0.35.0",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "postcss": "8.3.9",
    "postcss-loader": "6.2.0",
    "prettier": "2.4.1",
    "sass": "1.43.2",
    "sass-loader": "12.2.0",
    "storybook-addon-styled-component-theme": "2.0.0",
    "style-loader": "3.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "stylelint-scss": "3.21.0",
    "terser-webpack-plugin": "5.2.4",
    "ts-jest": "27.0.6",
    "ts-loader": "9.2.6",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "ttypescript": "1.5.12",
    "typescript": "4.4.4",
    "typescript-plugin-styled-components": "2.0.0",
    "webpack": "5.58.2",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "4.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not IE 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,scss,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc --fix",
      "echo test"
    ],
    "*.{jsx,tsx}": [
      "npm run stylelint:styled"
    ],
    "**/*.scss": [
      "npm run stylelint:scss"
    ]
  }
}
